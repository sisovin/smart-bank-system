{
     "name": "peanech/smart-bank-system",
    "description": "A Smart Banking System web application built with PHP.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "peanech Team",
            "email": "info@smartbank.peanech.online"
        }
    ],
    "require": {
        "php": ">=8.4.0",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "mpdf/mpdf": "^8.2",
        "firebase/php-jwt": "^6.0",
        "predis/predis": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Controllers\\": "app/controllers/",
            "App\\Models\\": "app/models/",
            "App\\Core\\": "app/core/",
            "App\\Middleware\\": "app/middleware/",
            "App\\Config\\": "app/config/",
            "App\\Helpers\\": "app/helpers/"
        },
        "classmap": [
            "app/models/",
            "app/controllers/",
            "app/middleware/",
            "config/"
        ],
        "files": []
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.4.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload --optimize"
        ],
        "post-update-cmd": [
            "@composer dump-autoload --optimize"
        ]
    }
}